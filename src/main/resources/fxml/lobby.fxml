<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.client.LobbyController"
      alignment="CENTER" spacing="20" padding="30">

    <Button text="Play" fx:id="playButton" prefWidth="200"/>
    <Button text="See Leaderboard" fx:id="leaderboardButton" prefWidth="200"/>
    <Button text="Logout" fx:id="LogoutButton" prefWidth="200"/>

</VBox>
