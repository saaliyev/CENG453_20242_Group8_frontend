<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.client.LobbyController"
      alignment="CENTER"
      spacing="20">

    <padding>
        <Insets top="50" bottom="50" left="50" right="50"/>
    </padding>

    <Button text="Play" onAction="#onPlayClicked" style="-fx-font-size: 18px;"/>
    <Button text="Logout" onAction="#onLogout" />
    <Text fx:id="messageText" />



</VBox>
